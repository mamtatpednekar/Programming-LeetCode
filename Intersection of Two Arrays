class Solution(object):
    def intersection(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        output = set()
        for i in nums1:
            if i in nums2:
                output.add(i)
            else:
                continue
        return list(output)
