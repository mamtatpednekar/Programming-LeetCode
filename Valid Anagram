class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        tsorted = str(sorted(t))
        ssorted = str(sorted(s))
        i = 0
        j = 0
        if tsorted == ssorted:
            return True
        else:
            return False
            
