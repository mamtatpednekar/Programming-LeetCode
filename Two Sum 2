class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for i in range(len(nums)):
            var = nums[i]
            for j in range(len(nums)):
                if var + nums[j]==target and i!=j:
                    lst=list()
                    return i,j
