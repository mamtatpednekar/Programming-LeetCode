class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        #nums.sort()
        #i=0
        #while i <= len(nums):
         #   if nums[i] == nums[i+1]:
          #      i = i + 2
           # else:
            #    return nums[i]
        
        for i in nums:
            if nums.count(i) == 1:
                return i
