class Solution(object):
    def sumZero(self, n):
        """
        :type n: int
        :rtype: List[int]
        """
        output = []
        if n%2 == 0:
            output = []
        else:
            output = [0]
        for i in range(1, n//2 + 1):
            output.append(+i)
            output.append(-i)
        return output
                
        
