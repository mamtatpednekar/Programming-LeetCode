class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        s = str(s)
        s = ' '.join(filter(str.isalnum, s))
        s = s.lower()
        rev = s[::-1]
        if s == rev:
            return True
        else:
            return False
                
        
        
